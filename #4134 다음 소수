import sys
input  = sys.stdin.readline
def primenumber(x):
    if x<2:
        return False
    if x==2:
        return True
    if x%2 == 0:
        return False
    else:#홀수만 검사하는데 제곱근의 절반보다 작은 수들로 나누었을때 안나눠지면 그냥 소수인거임
        for i in range(3,int(x**0.5)+1,2):
            if x%i == 0:
                return False
        return True
n = int(input())
for i in range(n):
    num = int(input())
    while(1):
        if primenumber(num):
           print(num)
           break
        num+=1
