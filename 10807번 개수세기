arr = []

n = int(input())

# for i in range(n):
    # arr.append(list(map(int,input().split()))) 이건 2차원 배열
arr.extend(map(int,input().split()))#이렇게 하면 반복문 없이 띄어쓰기로 받은 정수들을 배열에 넣어줌
a = int(input())
s = 0
for i in range(n):
    if(arr[i]== a):
        s+=1
print(s)
#COUNT를 사용하고list를 사용하여 1차원 배열을 바로 생성하는것이다 배열 메소드 잘 기억하자
n = int(input())
n_list = list(map(int,input().split()))#이렇게해서 1차원 배열을 생성해준다
v = int(input())
print(n_list.count(v))#count를 사용함으로써 몇개인지 찾을 수 있다
